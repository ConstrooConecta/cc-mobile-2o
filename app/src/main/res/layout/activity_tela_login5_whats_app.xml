<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".loginPath.TelaLogin5_SMS">

    <Button
        android:id="@+id/ContinuarWhatsapp"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="63dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/btncadastro_main_design"
        android:text="Continuar"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_digit2" />

    <Button
        android:id="@+id/EnviarNovamenteWhatsapp"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/btn_enviar_novamente"
        android:backgroundTint="#3385B3BD"
        android:text="Enviar novamente"
        android:textColor="#0A262C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ContinuarWhatsapp" />

    <Button
        android:id="@+id/UsarOutroMetodoWhatsapp"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="202dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#FFFFFF"
        android:text="Usar outro método"
        android:textColor="#0A262C"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EnviarNovamenteWhatsapp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

    <EditText
        android:id="@+id/et_digit2"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toStartOf="@+id/et_digit3"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/et_digit1"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toStartOf="@+id/et_digit2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/et_digit4"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toStartOf="@+id/et_digit5"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/et_digit3"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toStartOf="@+id/et_digit4"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/et_digit5"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toStartOf="@+id/et_digit6"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/et_digit6"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/btn_segunda_validacao"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="123dp"
        android:layout_marginEnd="73dp"
        android:text="Insira o código de verificação"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="43dp"
        android:text="Você recebeu um código de 6 dígitos por Whatsapp."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />
</androidx.constraintlayout.widget.ConstraintLayout>